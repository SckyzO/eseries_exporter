{
  "title": "NetApp E-Series - Balanced Overview",
  "uid": "eseries-balanced",
  "tags": [
    "netapp",
    "eseries",
    "storage",
    "balanced"
  ],
  "timezone": "",
  "schemaVersion": 41,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      },
      {
        "name": "storage_system",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(eseries_storage_system_status, instance)",
        "refresh": 1
      },
      {
        "name": "controller",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(eseries_controller_cpu_average_utilization_ratio{instance=\"$storage_system\"}, controller)",
        "allValue": ".*",
        "includeAll": true,
        "refresh": 1
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "System Health",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "title": "System Status",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 4,
        "h": 3
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "eseries_storage_system_status{instance=\"$storage_system\",status=\"optimal\"}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "OK",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "title": "Failed Components",
      "type": "stat",
      "gridPos": {
        "x": 4,
        "y": 1,
        "w": 4,
        "h": 3
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(eseries_drive_status{instance=\"$storage_system\",status!=\"optimal\"}) + sum(eseries_power_supply_status{instance=\"$storage_system\",status!=\"optimal\"}) or vector(0)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 1
              },
              {
                "color": "red",
                "value": 3
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "title": "Total IOPS",
      "type": "stat",
      "gridPos": {
        "x": 8,
        "y": 1,
        "w": 4,
        "h": 3
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(rate(eseries_controller_iops_total{instance=\"$storage_system\"}[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "iops",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 5000
              },
              {
                "color": "red",
                "value": 10000
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "title": "Avg Latency",
      "type": "stat",
      "gridPos": {
        "x": 12,
        "y": 1,
        "w": 4,
        "h": 3
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "avg(eseries_controller_combined_response_time_seconds{instance=\"$storage_system\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.01
              },
              {
                "color": "red",
                "value": 0.05
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "title": "Cache Hit Ratio",
      "type": "gauge",
      "gridPos": {
        "x": 16,
        "y": 1,
        "w": 8,
        "h": 3
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "100 * sum(rate(eseries_controller_cache_hits_iops_total{instance=\"$storage_system\"}[5m])) / sum(rate(eseries_controller_iops_total{instance=\"$storage_system\"}[5m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100,
          "min": 0,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 50
              },
              {
                "color": "green",
                "value": 80
              }
            ]
          }
        }
      }
    },
    {
      "type": "row",
      "title": "Performance Metrics",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "title": "IOPS by Controller",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 5,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum by (controller_label) (rate(eseries_controller_read_iops_total{instance=\"$storage_system\",controller=~\"$controller\"}[5m]))",
          "legendFormat": "Ctrl {{controller_label}} - Read"
        },
        {
          "expr": "sum by (controller_label) (rate(eseries_controller_write_iops_total{instance=\"$storage_system\",controller=~\"$controller\"}[5m]))",
          "legendFormat": "Ctrl {{controller_label}} - Write"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          },
          "unit": "iops"
        }
      },
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "title": "Response Time",
      "type": "timeseries",
      "gridPos": {
        "x": 12,
        "y": 5,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "eseries_controller_read_response_time_seconds{instance=\"$storage_system\",controller=~\"$controller\"}",
          "legendFormat": "Ctrl {{controller_label}} - Read"
        },
        {
          "expr": "eseries_controller_write_response_time_seconds{instance=\"$storage_system\",controller=~\"$controller\"}",
          "legendFormat": "Ctrl {{controller_label}} - Write"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "thresholdsStyle": {
              "mode": "line+area"
            }
          },
          "unit": "s",
          "thresholds": {
            "steps": [
              {
                "color": "transparent",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.01
              },
              {
                "color": "red",
                "value": 0.05
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "row",
      "title": "Hardware Components",
      "gridPos": {
        "x": 0,
        "y": 13,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "title": "Drives",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "sum(eseries_drive_status{instance=\"$storage_system\",status=\"optimal\"})",
          "legendFormat": "Optimal"
        },
        {
          "expr": "sum(eseries_drive_status{instance=\"$storage_system\",status!=\"optimal\"}) or vector(0)",
          "legendFormat": "Failed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Failed"
            },
            "properties": [
              {
                "id": "thresholds",
                "value": {
                  "steps": [
                    {
                      "color": "green",
                      "value": null
                    },
                    {
                      "color": "red",
                      "value": 1
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "title": "Power Supplies",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 14,
        "w": 4,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "min(eseries_power_supply_status{instance=\"$storage_system\",status=\"optimal\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "FAILED",
                  "color": "red"
                },
                "1": {
                  "text": "OK",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "title": "Fans",
      "type": "stat",
      "gridPos": {
        "x": 10,
        "y": 14,
        "w": 4,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "min(eseries_fan_status{instance=\"$storage_system\",status=\"optimal\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "FAILED",
                  "color": "red"
                },
                "1": {
                  "text": "OK",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "title": "Batteries",
      "type": "stat",
      "gridPos": {
        "x": 14,
        "y": 14,
        "w": 5,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "min(eseries_battery_status{instance=\"$storage_system\",status=~\"optimal|learning\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "ISSUE",
                  "color": "red"
                },
                "1": {
                  "text": "OK",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "title": "Cache Memory",
      "type": "stat",
      "gridPos": {
        "x": 19,
        "y": 14,
        "w": 5,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "min(eseries_cache_memory_dimm_status{instance=\"$storage_system\",status=\"optimal\"})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "FAILED",
                  "color": "red"
                },
                "1": {
                  "text": "OK",
                  "color": "green"
                }
              }
            }
          ],
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "green",
                "value": 1
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background"
      }
    },
    {
      "title": "Controller CPU Utilization",
      "type": "timeseries",
      "gridPos": {
        "x": 0,
        "y": 18,
        "w": 24,
        "h": 7
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "expr": "eseries_controller_cpu_average_utilization_ratio{instance=\"$storage_system\",controller=~\"$controller\"}",
          "legendFormat": "Ctrl {{controller_label}} - Avg"
        },
        {
          "expr": "eseries_controller_cpu_max_utilization_ratio{instance=\"$storage_system\",controller=~\"$controller\"}",
          "legendFormat": "Ctrl {{controller_label}} - Max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "unit": "percentunit",
          "max": 1,
          "min": 0,
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.7
              },
              {
                "color": "red",
                "value": 0.9
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "calcs": [
            "mean",
            "max",
            "lastNotNull"
          ],
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ]
}
